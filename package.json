{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev-all": "concurrently \"npm --section=backend run development\" \"npm --section=frontend run development\"  --kill-others-on-fail",
        "dev-all:watch": "concurrently \"npm --section=backend run development -- --watch\" \"npm --section=frontend run development -- --watch\"  --kill-others-on-fail",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod-all": "concurrently \"npm --section=backend run prod\" \"npm --section=frontend run prod\"  --kill-others-on-fail",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "lint:css": "stylelint resources/sass/**/*.scss",
        "lint:css:fix": "stylelint resources/sass/**/*.scss --fix",
        "lint:js": "eslint resources/js/**/*.{js,vue}",
        "lint:js:fix": "eslint resources/js/**/*.{js,vue} --fix",
        "format:js": "prettier resources/js/**/*.{js,vue} -c",
        "format:css": "prettier resources/sass/**/*.scss -c",
        "format:js:fix": "prettier resources/js/**/*.{js,vue} -w",
        "format:css:fix": "prettier resources/sass/**/*.scss -w",
        "blade": "blade-formatter resources/views/**/*.blade.php -c",
        "blade:fix": " blade-formatter resources/views/**/*.blade.php -w"
    },
    "devDependencies": {
        "axios": "^0.19",
        "babel-eslint": "^10.1.0",
        "blade-formatter": "^1.9.0",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0",
        "eslint": "^7.12.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jquery": "^1.5.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-vue": "^7.1.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.19",
        "prettier": "^2.1.2",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "stylelint": "^13.7.2",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recess-order": "^2.3.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-csstree-validator": "^1.9.0",
        "stylelint-rscss": "^0.4.0",
        "stylelint-scss": "^3.18.0",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "vue": "^2.6.12",
        "vue-router": "^3.4.8"
    }
}
